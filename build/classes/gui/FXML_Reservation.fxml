                  <ImageView fitHeight="28.0" fitWidth="35.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@images/icons8-reservation-100.png" />
                     </image>
                  </ImageView>
               </graphic>
               <padding>
                  <Insets left="70.0" />
               </padding>
